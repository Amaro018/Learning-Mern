// { "version": 2, "rewrites": [{ "source": "/(.*)", "destination": "/api" }] }

{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/"
        }
    ]
}